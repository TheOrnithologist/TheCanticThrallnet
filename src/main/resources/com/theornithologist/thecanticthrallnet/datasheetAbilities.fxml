<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.WebView?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.theornithologist.thecanticthrallnet.controllers.DatasheetAbilityController">
    <TitledPane fx:id="abilityName" expanded="false" styleClass="datasheet_item" alignment="CENTER">
        <VBox>
            <Label fx:id="abilityParameter" visible="false" styleClass="datasheet_item"/>
            <WebView fx:id="abilityDescription" style="-fx-pref-height: 75;"/>
        </VBox>
    </TitledPane>
</VBox>
